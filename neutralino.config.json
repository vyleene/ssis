{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "js.ssis",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/src/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "filesystem.*",
    "window.*"
  ],
  "globalVariables": {},
  "modes": {
    "window": {
      "title": "Simple Student Information System",
      "width": 1300,
      "height": 905,
      "minWidth": 400,
      "minHeight": 200,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/src/img/appIcon.png",
      "enableInspector": false,
      "borderless": true,
      "maximize": false,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": true,
      "useSavedState": false
    },
    "browser": {},
    "cloud": {},
    "chrome": {}
  },
  "cli": {
    "binaryName": "ssis",
    "resourcesPath": "/src/",
    "extensionsPath": "/ext/",
    "clientLibrary": "/src/js/neutralino/neutralino.js",
    "binaryVersion": "6.5.0",
    "clientVersion": "6.5.0"
  }
}